+              GButton(
+                icon: Icons.circle,
+                text: "Tumors",
+                leading: Image.asset('assets/tumors/tumor.png', width: 30, height: 30),
+              ),
+              GButton(
+                icon: Icons.circle,
+                text: "Credits",
+                leading: Image.asset('assets/tumors/people.png', width: 30, height: 30),
+              ),
+            ],
+          ),
+        ),
+      ),
+    );
+  }
+}
